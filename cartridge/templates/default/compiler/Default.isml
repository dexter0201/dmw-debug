*{box-sizing:border-box;margin:0;padding:0}html,body{height:100%}body{font-family:'Open Sans', sans-serif}.clr{clear:both}.dws-header,.dws-footer{height:60px;left:0;padding:20px;position:fixed;width:100%;z-index:10}.dws-header{background-color:#25B1E4;height:40px;padding:7px 20px;top:0}.dws-header h4{color:#fff;font-weight:600;letter-spacing:0.1rem;margin:0;text-align:center}.dws-footer{background-color:#fff;border-top:2px solid #25B1E4;bottom:0;padding:11px 20px}.dws-container{padding:40px 0 0;height:94%}.dws-container [class*="dws-col-"]{float:left;padding:15px 15px}.dws-container .dws-col-1{width:49%;margin-right:1%}.dws-container .dws-col-2{width:10%}.dws-container .dws-col-3{width:49%;margin-left:1%}.dws-container .dws-col-1,.dws-container .dws-col-3{background-color:#fff;height:100%;position:relative;z-index:1}.dws-container .dws-col-1:hover .dws-title,.dws-container .dws-col-3:hover .dws-title{filter:progid:DXImageTransform.Microsoft.Alpha(enabled=false);opacity:1}.dws-container .dws-title{filter:progid:DXImageTransform.Microsoft.Alpha(Opacity=10);opacity:0.1;-moz-transition:opacity 0.2s ease-in;-o-transition:opacity 0.2s ease-in;-webkit-transition:opacity 0.2s ease-in;transition:opacity 0.2s ease-in;background-color:#25B1E4;color:#fff;font-weight:bold;font-size:15px;padding:5px 50px;position:absolute;right:0;top:0;z-index:5}.dws-container .dws-title.dws-title-output{right:0}.dws-container .dws-title.dws-title-input{right:0px}.btn-tip{float:left;font-size:12px}.btn-tip p{margin:0}.btn-tip p span{color:#25B1E4}.dws-button{-moz-border-radius:2px;-webkit-border-radius:2px;border-radius:2px;background-color:#25B1E4;border:none;color:#fff;cursor:pointer;display:block;float:left;font-weight:600;margin-right:20px;padding:8px 0;text-align:center;width:100px;transition:all .1s ease-in}.dws-button:hover{background-color:#1892be}.dws-button.btn-clear{background-color:#EA5555}.dws-button.btn-clear:hover{background-color:#e42828}.input-wrapper{position:relative;z-index:1;height:100%}.CodeMirror{-moz-border-radius:2px;-webkit-border-radius:2px;border-radius:2px;height:100%;border:1px solid #dcdcdc;z-index:1}.compile-output-panel{-moz-border-radius:2px;-webkit-border-radius:2px;border-radius:2px;border:1px solid #dcdcdc;height:100%;overflow-y:auto;padding:10px 5px;position:relative;z-index:1}.compile-output-panel .compiler-result{display:block;font-size:13px}.compile-output-panel .compiler-result:first-of-type{color:#13C4F9}.compile-output-panel .compiler-result:first-of-type:hover{color:#000}.compile-output-panel .compiler-result p{padding:3px 5px}.compile-output-panel .compiler-result p:nth-of-type(even){background-color:#efefef}.compile-output-panel .compiler-result p:hover{background-color:#EAEAEA}.compile-output-panel .compiler-result p.error{background-color:#FFC2C2;color:#841313}.compile-output-panel .compiler-result p.log{font-style:italic}.compile-output-panel .compiler-result p.success{background-color:#DBFFC2;color:#367B05}.compile-output-panel,.CodeMirror{border-top:4px solid #25B1E4}.compile-output-panel-text{background-color:#fff;font-size:12px; padding-left:6px;}.ajaxImg{width:48px;height:48px;position:absolute;z-index:10;margin-left:-24px;margin-top:-24px;left:50%;top:42%}@media (max-width: 800px){.dws-container [class*="dws-col-"]{float:none;padding:15px 15px}.dws-container .dws-col-1{width:100%;margin-right:0}.dws-container .dws-col-3{width:100%;margin-left:0}.dws-container .dws-col-1,.dws-container .dws-col-3{background-color:#fff;height:50%}}@media (max-width: 600px){.btn-tip{display:none;visibility:hidden}}
.result{clear:both;margin:6px}.result .error{background-color:#FFC2C2};
.node{position:relative;clear:both;float:left;font-size:12pt;font-family:arial,sans-serif}.node .head{position:relative;display:block;float:left;clear:both;padding:6px}.node .head:hover{background-color:#eee}.node .head .controller{float:left;cursor:pointer;font-weight:700;font-size:13px;color:#777}.node .head .title{float:left}.node .head .title .number{color:red}.node .head .title .string{color:green}.node .head .title .boolean{color:#FF8C00}.node .head .title .null{color:#004ED0}.node .head .title .undefined{color:#004ED0}.node .body{position:relative;float:left;clear:both;padding-left:16px;border-left:1px dashed #bbb}.node .body.collapse{display:none}
</style>
</head>
<body>
    <div class="dws-header">
        <h4>DEMANDWARE SCRIPT ONLINE EDITOR</h4>
    </div>
    <div class="dws-container">
        <div class="dws-col-1">
            <div class="input-wrapper">
                <span class="dws-title dws-title-input">Input</span>
                  <textarea id="compiled-string"></textarea>
            </div>
        </div>
        <div class="dws-col-3">
            <div class="compile-output-panel">
                <span class="dws-title dws-title-output">Output</span>
                <div class="ajaxImg" style="display: none;"><img src="${URLUtils.staticURL('/img/ajax/gears.gif')}"></div>
                <div id="compile-output-panel-text" class="compile-output-panel-text">
                </div>
            </div>
        </div>
    </div>
    <div class="clr"></div>
    <div class="dws-footer">
        <div class="dws-action">
            <button id="btn-compile" class="dws-button" data-compile-url="${URLUtils.http('DW-Compile')}">Run</button>
            <button id="btn-clear" class="dws-button btn-clear">Clear</button>
            <div class="btn-tip">
                <p>Press <span><strong>Esc</strong></span> to clear the result </p>
                <p>Press <span><strong>Ctrl-Enter</strong></span> to compile the code </p>
            </div>
        </div>
    </div>
    
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
    <script src="${URLUtils.staticURL('/js/compiler/lib/codemirror.js')}"></script>
    <script src="${URLUtils.staticURL('/js/compiler/lib/javascript.js')}" type="text/javascript"></script>
    <script src="${URLUtils.staticURL('/js/compiler/lib/underscore-min.js')}" type="text/javascript"></script>
    <script src="${URLUtils.staticURL('/js/compiler/jsonNode.js')}" type="text/javascript"></script>
    <script src="${URLUtils.staticURL('/js/compiler/jsonNodeHtml.js')}" type="text/javascript"></script>
    <script src="${URLUtils.staticURL('/js/compiler/compiler.js')}" type="text/javascript"></script>
</body>